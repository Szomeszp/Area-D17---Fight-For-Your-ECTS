{
  "npc_main":
  {
    "paths": [
      {
        "id": 0,
        "main_text": "SIEMA, SIEMA, SIEMANKO",
        "number_of_sub_paths" : 2,
        "sub_paths": [
          {
            "text": "No siema, siema... Co tam?",
            "go_to": 1
          },
          {
            "text": "Siema... wiesz moze ktoredy na ceramike?",
            "go_to": 2
          }
        ]
      },

      {
        "id": 1,
        "main_text": "Siema, siema, a dobrze. A tam?",
        "number_of_sub_paths" : 1,
        "sub_paths": [
          {
            "text": "A też dobrze!",
            "go_to": -1
          }
        ]
      },

      {
        "id": 2,
        "main_text": "Na ceramike to nie tutaj!",
        "number_of_sub_paths" : 1,
        "sub_paths": [
          {
            "text": "Aha... dzieki...",
            "go_to": -1
          }
        ]
      }
    ]
  },

  "npc_one":
  {
    "paths":[
      {
        "main_text": "Baba, bleble, xuxu!",
        "number_of_sub_paths": 1,
        "sub_paths": [
          {
            "text": "Aha? Ok?",
            "go_to": -1
          }
        ]
      }
    ]
  },

  "npc_three":
  {
    "paths": [
      {
        "id": 0,
        "main_text": "Witam, czego szukasz?",
        "number_of_sub_paths" : 2,
        "sub_paths": [
          {
            "text": "Widziałeś może sekretny klucz?",
            "go_to": 1
          },
          {
            "text": "Niczego, przyszedłem się tylko rozglądnąć.",
            "go_to": -1
          }
        ]
      },

      {
        "id": 1,
        "main_text": "Tak, to ja go mam! Potrzebujesz go?",
        "number_of_sub_paths" : 2,
        "sub_paths": [
          {
            "text": "Tak, czy mógłbyś mi go dać?",
            "go_to": 2
          },
          {
            "text": "Nie, tak tylko pytam, do widzenia!",
            "go_to": -1
          }
        ]
      },

      {
        "id": 2,
        "main_text": "Weź go, ale używaj go z rozwagą!",
        "number_of_sub_paths" : 2,
        "sub_paths": [
          {
            "text": "Dzięki, tak tez zrobię!!!",
            "go_to": 1000
          },
          {
            "text": "Jednak, aż tak bardzo go nie potrzebuje.",
            "go_to": -1
          }
        ]
      }
    ]
  }
